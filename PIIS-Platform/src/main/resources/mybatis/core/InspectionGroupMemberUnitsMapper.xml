<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="tech.piis.modules.core.mapper.InspectionGroupMemberUnitsMapper">

    <resultMap type="InspectionGroupMemberUnitsPO" id="inspectionGroupMemberUnitsResult">
        <id property="groupMemberUnitsId" column="group_member_units_id"/>
        <result property="groupMemberId" column="group_member_id"/>
        <result property="unitsId" column="units_id"/>
        <result property="groupId" column="group_id"/>
    </resultMap>

    <insert id="insertGroupMemberUnitsBatch" parameterType="java.util.List">
        insert into inspection_group_member_units(group_member_id,units_id,group_id) values
        <foreach item="item" index="index" collection="list" separator=",">
            (#{item.groupMemberId},#{item.unitsId},#{item.groupId})
        </foreach>
    </insert>

</mapper>