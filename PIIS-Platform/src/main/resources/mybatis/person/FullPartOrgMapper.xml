<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="tech.piis.modules.person.mapper.FullPartOrgMapper">
    <!--专兼职管理ResultMap-->
    <resultMap id="fullPartOrgResultMap" type="tech.piis.modules.person.domain.po.FullPartOrgPO">
        <id property="fullPartId" column="ID"/>
        <result property="orgId" column="ORG_ID"/>
        <result property="orgName" column="ORG_NAME"/>
        <!--<result property="isLeading" column="IS_LEADING"/>-->
        <result property="officeMode" column="OFFICE_MODE"/>
        <result property="fullNumber" column="FULL_NUMBER"/>
        <result property="partNumber" column="PART_NUMBER"/>
        <result property="createdBy" column="CREATED_BY"/>
        <result property="createdTime" column="CREATED_TIME"/>
        <result property="updatedBy" column="UPDATED_BY"/>
        <result property="updatedTime" column="UPDATED_TIME"/>
        <result property="entId" column="ENT_ID"/>
    </resultMap>
    <!--sql语句-->
    <!--根据机构ID获取专兼职人数-->
    <select id="getFullPartNumByOrganId" resultMap="fullPartOrgResultMap">
        select FULL_NUMBER,PART_NUMBER FROM person_full_part_org where ORG_ID = #{orgId}
    </select>
</mapper>